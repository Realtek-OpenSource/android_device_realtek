##########################################################################################
# Set Neptune/Mars TP Interface                                                          #
# TP_MODEL : NONE / NEPTUNE_TP / NEPTUNE_TP_EX / MARS_TP                                 #
# TP_RESOURCE_PARTITION : form  1~63 (default : 32)                                      #
##########################################################################################
TP0_CHSEL = 0
TP1_CHSEL = 1
TP_RESOURCE_PARTITION       = 32
TP_SECTION_ASSEMBLER_ENABLE = Y

ifneq ($(findstring APCFG_HW_DTV, $(APCFG_FLAGS)),)
  ifeq ($(CHIP_MODEL), 0x1283)
    TP_MODEL = MARS_TP
  endif

  ifeq ($(BOARD_ID), RTD1283-QA)
    TP0_CHSEL = 0
    TP1_CHSEL = 2
  endif

  ifeq ($(CHIP_MODEL), 0x1185)
    TP_MODEL = JUPITER_TP
  endif

  ifeq ($(CHIP_MODEL), 0x1881)
    TP_MODEL = JUPITER_TP
  endif

  ifeq ($(CHIP_MODEL), 0x1186)
    TP_MODEL = SATURN_TP
  endif

else
  TP_MODEL  = NONE
endif


##########################################################################################
# Select External TVD Chip                                                               #
# Available ID:                                                                          #
# TVP5150,         TI TVP5150                                                            #
##########################################################################################
ifneq ($(findstring APCFG_HW_ANALOG_INPUT, $(APCFG_FLAGS)),)
  EXTERNAL_TVD = YES
  EXTERNAL_TVD_ID = TVP5150
