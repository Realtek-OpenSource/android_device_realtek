##########################################################################################
# Select DTV Tuner                                                                       #
#                                                                                        #
# DTV_FE_NONE                       : No Tuner                                           #
#                                                                                        #
# DTV_FE_RTL2830_FE664X             : MN & not MN & DVB-T                                #
# DTV_FE_RTL2830_FE6660X            : not MN      & DVB-T                                #
# DTV_FE_RTL2830_TDTG_D223D         :               DVB-T                                #
# DTV_FE_RTL2830_SN5000             : MN & not MN & DVB-T                                #
# DTV_FE_RTL2830_TDTC_GX31D         : DVB-T                                              #
# DTV_FE_RTL2830_TDAD3_C01A         : DVB-T                                              #
# DTV_FE_RTL2830_TDAG1_A02A         : not MN      & DVB-T                                #
#                                                                                        #
# DTV_FE_TDA10048_FQD1216           : not MN      & DVB-T                                #
#                                                                                        #
# DTV_FE_RTL2810_DTT76831           : MN & ATSC & OpenCable                              #
# DTV_FE_RTL2810_DPT78130           : MN & ATSC & OpenCable                              #
# DTV_FE_RTL2810_TDQU4_507A         : MN & ATSC                                          #
#                                                                                        #
# DTV_FE_S5H1411X_DTT76831          : MN & ATSC & OpenCable                              #
# DTV_FE_S5H1411X_SN5000            : MN & not MN & ATSC & OpenCable                     #
# DTV_FE_S5H1411X_TDAU1_D01A        : ATSC & OpenCable                                   #
# DTV_FE_S5H1411X_TDAU3_B01A        : MN & ATSC & OpenCable                              #
#                                                                                        #
# DTV_FE_LGS8G52_DTCS403PH172A      : DTMB & PAL                                         #
#                                                                                        #
# ATV_FE_TMI1C23I1W                 : MN & notMN         CHANGHONG                       #
# ATV_FE_TCSX0601XD25X              : notMN              SAMSUNG                         #
# ATV_FE_TCSN9082DA25A              : MN                 SAMSUNG                         #
# ATV_FE_FQD12X6L                   : MN & notMN         PHILIPS                         #
# ATV_FE_CDT5GN14                   : MN                 CHINADRAGON                     #
#                                                                                        #
# DTV_FE_TPIF_ONLY                  : ALL (pseude device)                                #
##########################################################################################
ifneq ($(findstring APCFG_HW_DTV, $(APCFG_FLAGS)),)

   SENSITIVITY_DB_EN= Y
#  TP_INVERSE_ORDER = Y

  ifeq ($(BUILD_DVB_T), Y)
    FE_MODELS  = DTV_FE_RTL2832_MXL5007T
    FE_MODELS += DTV_FE_RTL2832_TDA18272_DUAL
    FE_MODELS += DTV_FE_NIM_MXL5007
    #TP0_FE     = DTV_FE_RTL2832_MXL5007T
    #TP1_FE     = DTV_FE_RTL2832_MXL5007T
    TP0_FE     = DTV_FE_RTL2832_TDA18272_DUAL
    TP1_FE     = DTV_FE_RTL2832_TDA18272_DUAL
  endif

  ifeq ($(BUILD_DVB_S), Y)
    TP0_FE     = DTV_FE_NIM_AVL6211_AV2011
    TP1_FE     = DTV_FE_NIM_AVL6211_AV2011
    TP1_RF_INPUT_INTERNAL_LOOPTHROUGH = Y
  endif

  ifeq ($(BUILD_DVB_C), Y)
    TP0_FE     = DTV_FE_RTL2840_TDA18272_DUAL
    TP1_FE     = DTV_FE_RTL2840_TDA18272_DUAL
  endif

  ifeq ($(BUILD_ATSC_T), Y)
    TP0_FE     = DTV_FE_S5H1411X_TDAU1_D01A
    TP1_FE     = DTV_FE_NONE
  endif

  TUNER_0_I2C_CHANNEL = 1
  TUNER_1_I2C_CHANNEL = 1

  ifeq ($(BOARD_ID), RTD1283-QA)
    TUNER_0_I2C_CHANNEL = 0
    TUNER_1_I2C_CHANNEL = 1
  endif
else
  TP0_FE     = DTV_FE_NONE
  TP1_FE     = DTV_FE_NONE

  TUNER_0_I2C_CHANNEL = 0
  TUNER_1_I2C_CHANNEL = 0
endif

